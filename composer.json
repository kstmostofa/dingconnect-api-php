{
    "name": "kstmostofa/dingconnect-api-php",
    "description": "A PHP Client for the  DingConnect Mobile Top-Up API.",
    "type": "library",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "dingconnect"
    ],
    "homepage": "https://github.com/kstmostofa/dingconnect-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Mostafijur Rahman",
            "email": "mostofa122@gmail.com",
            "homepage": "https://mostofa.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.29",
        "squizlabs/php_codesniffer": "3.7.2",
        "friendsofphp/php-cs-fixer": "3.34.0"
    },
    "autoload": {
        "psr-4": { "Kstmostofa\\DingConnect\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Kstmostofa\\DingConnect\\" : "test/" }
    }
}
